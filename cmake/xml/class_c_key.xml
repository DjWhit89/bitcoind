      <memberdef kind="function" id="class_c_key_1ab813495ee5dd24d04ccd4abe59b6cf50" prot="public" static="no" const="yes" explicit="no" inline="no" virt="non-virtual">
        <type>bool</type>
        <definition>bool SignCompact</definition>
        <argsstring>(const uint256 &amp;hash, std::vector&lt; unsigned char &gt; &amp;vchSig) const</argsstring>
        <name>SignCompact</name>
        <qualifiedname>CKey::SignCompact</qualifiedname>
        <param>
          <type>const <ref refid="classuint256" kindref="compound">uint256</ref> &amp;</type>
          <declname>hash</declname>
